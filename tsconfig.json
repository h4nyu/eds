{
  "compilerOptions": {
    "target": "esnext", 
    "module": "esnext",
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny":false,
    "noImplicitThis":false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowJs": false
  }
}
