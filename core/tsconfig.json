{
  "compilerOptions": {
    "module": "ESNext", 
    "target": "ESNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noEmit": true,
    "allowJs": false
  }
}
